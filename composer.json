    "require": {
        "php": "^7.4",
        "ext-parallel": "*",
        "moneyphp/money": "^3.2.1",
        "ocramius/package-versions": "^1.4",
        "react-parallel/contracts": "^1.0",
        "react-parallel/event-loop": "^1.1",
        "react/event-loop": "^1.1",
        "react/promise": "^2.7",
        "thecodingmachine/safe": "^1.3",
        "wyrihaximus/async-test-utilities": "^3.3",
        "wyrihaximus/iterator-or-array-to-array": "^1.1",
        "wyrihaximus/pool-info": "^1.0"
    },
    "require-dev": {},
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "extra": {
        "unused": [
            "ext-parallel"
        ]
    },
    "autoload": {
        "psr-4": {
            "ReactParallel\\Tests\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
