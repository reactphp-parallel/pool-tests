{
    "name": "react-parallel/pool-tests",
    "description": "\ud83c\udfb1 Pool tests for bridging ext-parallel and ReactPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "https://www.wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-parallel": "*",
        "moneyphp/money": "^3.2.1",
        "react-parallel/contracts": "^2@dev",
        "react-parallel/event-loop": "^2@dev",
        "react/event-loop": "^1.5",
        "react/promise": "^3.1",
        "wyrihaximus/async-test-utilities": "^7.2",
        "wyrihaximus/pool-info": "^2.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "ReactParallel\\Tests\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.2.13"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
